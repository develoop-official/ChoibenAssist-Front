{
  "name": "choiben-assist-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen && panda",
    "dev": "concurrently \"npm run dev:panda\" \"npm run dev:next\"",
    "dev:panda": "panda --watch",
    "dev:next": "next dev --turbopack",
    "build": "npm run build:prepare && npm run build:panda && npm run build:next",
    "build:prepare": "NODE_ENV=production npm run prepare",
    "build:panda": "panda --minify",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.5",
    "next": "15.3.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@pandacss/dev": "^0.54.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "typescript": "^5"
  }
}
